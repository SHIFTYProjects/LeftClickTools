            logAction "Attempted to access KMS Info."
            $outputBox.AppendText("-------------------------------------------------------------------------------------------------" + [Environment]::NewLine)
            $outputBox.AppendText("Attempted to access KMS Info." + [Environment]::NewLine)


            #INSERT SCRIPT TO PERFORM HERE

                        
                        
          }
