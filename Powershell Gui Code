function KMS {
  logAction "KMS Tool Started."
  $outputBox.AppendText("KMS Tool Started." + [Environment]::NewLine)
  $msgBoxInput = $wshell.Popup("Do you want to Run KMS check on PC(s)?", 0, "Confirm", 0x1)
